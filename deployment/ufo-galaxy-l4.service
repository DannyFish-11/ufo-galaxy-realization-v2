[Unit]
Description=UFO Galaxy L4 Autonomous Intelligence System
After=network.target

[Service]
Type=simple
User=ubuntu
WorkingDirectory=/home/ubuntu/code_audit/ufo-galaxy-realization
ExecStart=/usr/bin/python3 /home/ubuntu/code_audit/ufo-galaxy-realization/start_l4.py
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal

# 环境变量
Environment="PYTHONUNBUFFERED=1"

[Install]
WantedBy=multi-user.target
