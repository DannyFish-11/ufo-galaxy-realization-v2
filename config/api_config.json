{
  "metadata": {
    "version": "1.1",
    "description": "Galaxy API 配置 - 完整版",
    "last_updated": "2026-02-21"
  },
  
  "strategy": {
    "type": "dual_parallel",
    "description": "双并行策略: OneAPI聚合器(核心) + 其他单独模型(细节)",
    "priority": {
      "oneapi": 1,
      "direct": 2
    }
  },
  
  "oneapi": {
    "enabled": true,
    "description": "OneAPI 聚合器 - 核心供给",
    "base_url": "http://localhost:8001/v1",
    "api_key": "",
    "models": [
      {"id": "gpt-4o", "name": "GPT-4o", "provider": "openai"},
      {"id": "gpt-3.5-turbo", "name": "GPT-3.5 Turbo", "provider": "openai"},
      {"id": "claude-3-5-sonnet", "name": "Claude 3.5 Sonnet", "provider": "anthropic"},
      {"id": "deepseek-chat", "name": "DeepSeek Chat", "provider": "deepseek"},
      {"id": "glm-4", "name": "GLM-4", "provider": "zhipu"},
      {"id": "llama-3.3-70b", "name": "Llama 3.3 70B", "provider": "meta"}
    ]
  },
  
  "direct_models": {
    "description": "直接模型 - 细节供给",
    
    "openai": {
      "enabled": true,
      "api_key": "",
      "base_url": "https://api.openai.com/v1",
      "models": ["gpt-4o", "gpt-4-turbo", "gpt-3.5-turbo"],
      "env_key": "OPENAI_API_KEY"
    },
    
    "anthropic": {
      "enabled": true,
      "api_key": "",
      "base_url": "https://api.anthropic.com/v1",
      "models": ["claude-3-5-sonnet-20241022", "claude-3-opus-20240229"],
      "env_key": "ANTHROPIC_API_KEY"
    },
    
    "deepseek": {
      "enabled": true,
      "api_key": "",
      "base_url": "https://api.deepseek.com/v1",
      "models": ["deepseek-chat", "deepseek-coder"],
      "env_key": "DEEPSEEK_API_KEY"
    },
    
    "zhipu": {
      "enabled": true,
      "api_key": "",
      "base_url": "https://open.bigmodel.cn/api/paas/v4",
      "models": ["glm-4", "glm-4-flash", "glm-4-plus"],
      "env_key": "ZHIPU_API_KEY"
    },
    
    "groq": {
      "enabled": true,
      "api_key": "",
      "base_url": "https://api.groq.com/openai/v1",
